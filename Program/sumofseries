//Write a C++ program to find the sum of series 1+2+3+....+n.
#include<iostream>
#include<conio.h>
using namespace std;
int main()
{
    int i,n,sum = 0;
    cout<<"1+2+3+...+n";
    cout<<"\nEnter the value of n:";
    cin>>n;
    for(int i = 1;i<=n;i++)
    {
        sum+=i;
    }
    cout<<"SUM"<<sum;
    return 0;
}
